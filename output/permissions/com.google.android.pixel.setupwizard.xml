    <permission name="com.google.android.providers.gsf.permission.READ_GSERVICES"/>
    <permission name="com.google.android.setupwizard.PARTNER_SETUP"/>
    <permission name="com.google.android.setupwizard.SETUP_COMPAT_SERVICE"/>
