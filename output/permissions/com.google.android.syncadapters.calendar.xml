<!--
This XML file declares which signature|privileged permissions should be granted to privileged
applications on GMS or Google-branded devices.
It allows additional grants on top of privapp-permissions-platform.xml
-->
<permissions>
  <privapp-permissions package="com.google.android.syncadapters.calendar">
    <permission name="android.permission.USE_CREDENTIALS"/>
    <permission name="android.permission.READ_SYNC_STATS"/>
    <permission name="android.permission.READ_SYNC_SETTINGS"/>
    <permission name="android.permission.WRITE_SYNC_SETTINGS"/>
    <permission name="android.permission.INTERNET"/>
    <permission name="com.google.android.googleapps.permission.GOOGLE_AUTH"/>
    <permission name="android.permission.SUBSCRIBED_FEEDS_READ"/>
    <permission name="android.permission.SUBSCRIBED_FEEDS_WRITE"/>
    <permission name="android.permission.READ_CALENDAR"/>
    <permission name="android.permission.WRITE_CALENDAR"/>
    <permission name="com.google.android.providers.gsf.permission.READ_GSERVICES"/>
    <permission name="android.permission.ACCESS_NETWORK_STATE"/>
    <permission name="com.google.android.c2dm.permission.RECEIVE"/>
    <permission name="android.permission.RECEIVE_BOOT_COMPLETED"/>
    <permission name="android.permission.WAKE_LOCK"/>
    <permission name="com.google.android.syncadapters.calendar.DYNAMIC_RECEIVER_NOT_EXPORTED_PERMISSION"/>
  </privapp-permissions>
</permissions>
